						mySwiper1.autoplay.stop()
					}
					//鼠标覆盖停止自动切换与隐藏前进后退按钮
					mySwiper1.el.onmouseleave = function() {
						mySwiper1.autoplay.start()
					}
				}

				function userAgent() {
					$('#product').hide()
					var dom = $('<div id="newproduct" class="product">')
					dom.html($('#product').html())
					$('#banner').after(dom)
					$('#product').remove()
				}

				if (navigator.userAgent.indexOf("iPhone") > 0 || navigator.userAgent.indexOf("Android") > 0) {
					// 	<div id="product" class="product mobile">
					userAgent()
