			$(function() {
				var ua = navigator.userAgent.indexOf("iPhone") > 0 || navigator.userAgent.indexOf("Android") > 0
				var mySwiper = new Swiper('.swiper-container', {
					direction: 'horizontal', // 垂直切换选项
					loop: true, // 循环模式选项
					autoplay: {
						delay: 5000
					},
					// 如果需要分页器
					pagination: {
						el: '.swiper-pagination-banner',
						clickable: true,
						bulletClass: 'dot',
						bulletActiveClass: 'active',
					},

					// 如果需要前进后退按钮
					navigation: {
						nextEl: '.swiper-button-next',
						prevEl: '.swiper-button-prev',
					}

				})
				if (!ua) {
					//鼠标覆盖停止自动切换与隐藏前进后退按钮
					mySwiper.el.onmouseenter = function() {
						mySwiper.navigation.$nextEl.removeClass('swiper-hide');
						mySwiper.navigation.$prevEl.removeClass('swiper-hide');
						mySwiper.autoplay.stop()
					}
					//鼠标覆盖停止自动切换与隐藏前进后退按钮
					mySwiper.el.onmouseleave = function() {
						mySwiper.navigation.$nextEl.addClass('swiper-hide');
						mySwiper.navigation.$prevEl.addClass('swiper-hide');
						mySwiper.autoplay.start()
					}
				}


				var mySwiper1 = new Swiper('.swiper-container1', {
					direction: 'horizontal', // 垂直切换选项
					loop: true,
					slidesPerView: 1,
					loopedSlides: 5,
